<% include ../partials/header %>

<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Content</th>
        <th scope="col">Rating</th>
        <th scope="col">Rater</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach( (row) => { %>
        <tr>
          <th scope="row"><%= row.email %></th>
          <td><%= row.alias %></td>
          <td><%= row.observations %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<% include ../partials/footer %>
