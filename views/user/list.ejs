<% include ../partials/header %>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Alias</th>
        <th scope="col">Observations</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach( (row) => { %>
        <tr>
          <td><a href="/user/profile/<%= row.id %>"><%= row.alias %></a></td>
          <td><%= row.observationCount %></td>
        </tr>
      <% }) %>
      <tr class="primary">
        <th>Total</th>
        <td><%= total %></td>
      </tr>
    </tbody>
  </table>

</div>

<% include ../partials/footer %>
